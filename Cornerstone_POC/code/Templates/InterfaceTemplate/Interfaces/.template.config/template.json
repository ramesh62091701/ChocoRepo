{
  "$schema": "http://json.schemastore.org/template",
  "author": "Sonata-Software",
  "classifications": [ "Interface" ],
  "identity": "CSOD_Custom_Interface_Template",
  "name": "CSOD custom Interface template",
  "description": "Custom Interface template for CSOD",
  "shortName": "interfacecustomtemplate",
  "sourceName": "Interfaces",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0"
        },
        {
          "choice": "net8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "$$TargetFramework$$"
    }
  }
}
